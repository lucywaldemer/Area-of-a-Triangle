let string1 = 'a';
let string2 = 'aardvark';

function charCount (string1, string2){
  // break string2 apart into characters
  let array = string2.split('');
  // find string1 in array
  let numberOfChars= 0;
  for(let i = 0; i < array.length; i++){
    if(arr[i] === string1){
      numberOfChars++
    }
  }
  // print that number
  return nnumberOfChars;
}
